{
  "name": "invest-electron",
  "productName": "invest-electron",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start": "cross-env FLASK_APP=src/server.py npx electron .",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "test": "jest",
    "transpile": "npx babel src/* -d build/",
    "package": "electron-builder build --linux -c.extraMetadata.main=build/main.js --publish never"
  },
  "jest": {
    "collectCoverage": true
  },
  "keywords": [],
  "author": "dmf",
  "license": "MIT",
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "invest-gui"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ],
      "packagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "invest_electron"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "investelectron"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@turf/turf": "^5.1.6",
    "chroma-js": "^2.1.0",
    "cross-env": "^5.2.1",
    "esm": "^3.2.25",
    "font-awesome": "^4.7.0",
    "google-palette": "^1.1.0",
    "leaflet": "^1.6.0",
    "papaparse": "^5.1.1",
    "prop-types": "^15.7.2",
    "radium": "^0.25.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-easy-chart": "^1.0.0",
    "react-hot-loader": "^3.1.3",
    "react-lazylog": "^4.5.1",
    "react-leaflet": "^2.6.1",
    "react-leaflet-choropleth": "^2.0.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@jest-runner/electron": "^2.0.3",
    "@testing-library/dom": "^6.12.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "dotenv": "^8.2.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^2.2.4",
    "jest": "^24.8.0",
    "react": "^16.12.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.12.0",
    "react-vega": "^7.2.1",
    "sinon": "^9.0.1",
    "spectron": "^10.0.0",
    "tail": "^2.0.3"
  }
}
